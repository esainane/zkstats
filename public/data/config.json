{
  "src": "data/live.json",
  "orders": {
    "facorder": [
      "Cloakbot Factory",
      "Shieldbot Factory",
      "Spider Factory",
      "Jumpbot Factory",
      "Amphbot Factory",
      "Hovercraft Platform",
      "Rover Assembly",
      "Tank Foundry",
      "Gunship Plant",
      "Airplane Plant",
      "Shipyard",
      "Strider Hub",
      "Never",
      "Any"
    ]
  },
  "colors": {
    "fac": {
      "Cloakbot Factory": "#498fcb",
      "Shieldbot Factory": "#cb6e49",
      "Rover Assembly": "#d7a43e",
      "Hovercraft Platform": "#66af9a",
      "Spider Factory": "#68ad52",
      "Jumpbot Factory": "#ad527a",
      "Tank Foundry": "#413582",
      "Amphbot Factory": "#40cdcc",
      "Airplane Plant": "#eff632",
      "Gunship Plant": "#b2ee65",
      "Shipyard": "#6e8df0",
      "Strider Hub": "#ee0e08",
      "Never": "#ffffff"
    }
  },
  "columns": [
    {
      "type": "tiny",
      "charts": [
        {
          "vis": "count",
          "dim": "count",
          "dim_pretty": "Filter"
        },{
          "vis": "records",
          "dim": "gameid",
          "dim_pretty": "Battles",
          "count": 30,
          "columns": [
            {
              "label": "",
              "prop": "gameid",
              "type": "link",
              "ref": "<a href='https://zero-k.info/Battles/Detail/{}'>{}</a>"
            }
          ],
          "order": "descending"
        }
      ]
    },{
      "type": "wide",
      "charts": [
        {
          "vis": "bar",
          "dim": "started",
          "dates": true,
          "elasticX": true,
          "group": "Week",
          "dim_pretty": "Match start time",
          "verticalXAxisTicks": true
        },{
          "vis": "bar",
          "dim": "duration",
          "group": 900,
          "logarithmic": true,
          "elasticX": false,
          "dim_pretty": "Game Time (Frames, Logarithmic)",
          "verticalXAxisTicks": true
        },{
          "vis": "matchups",
          "dim": {
            "id": "matchups",
            "type": "triangle",
            "rows": "winner_fac",
            "cols": "loser_fac"
          },
          "verticalXAxisTicks": true,
          "order": "facorder",
          "dim_pretty": "Matchups",
          "show_any": "true",
          "size_by_popularity": false
        }
      ]
    },{
      "type": "thin",
      "charts": [
        {
          "vis": "pie",
          "dim": "map",
          "dim_pretty": "Matchmaking Map Used"
        },{
          "vis": "pie",
          "dim": "map_type",
          "dim_pretty": "Matchmaking Map Type"
        },{
          "vis": "pie",
          "dim": "mirror_match",
          "dim_pretty": "Mirror Match"
        },{
          "vis": "pie",
          "dim": "zk_version",
          "dim_pretty": "Zero-K Version",
          "sort_name": true
        },{
          "vis": "pie",
          "dim": "has_fullwhr",
          "dim_pretty": "Full WHR Availability",
          "when": "fullwhr"
        },{
          "vis": "pie",
          "dim": "spring_version",
          "dim_pretty": "Spring RTS Version",
          "when": "springver"
        }
      ]
    },{
      "type": "thin",
      "charts": [
        {
          "vis": "pie",
          "dim": "winner_fac1",
          "dim_pretty": "Winner's First Factory",
          "colors": "fac"
        },{
          "vis": "pie",
          "dim": "winner_fac2",
          "dim_pretty": "Winner's Second Factory",
          "colors": "fac"
        },{
          "vis": "pie",
          "dim": "winner_fac3",
          "dim_pretty": "Winner's Third Factory",
          "colors": "fac"
        },{
          "vis": "pie",
          "dim": "winner_fac4",
          "dim_pretty": "Winner's Fourth Factory",
          "colors": "fac"
        },{
          "vis": "pie",
          "dim": "winner_fac5",
          "dim_pretty": "Winner's Fifth Factory",
          "colors": "fac"
        }
      ]
    },{
      "type": "thin",
      "charts": [
        {
          "vis": "pie",
          "dim": "loser_fac1",
          "dim_pretty": "Loser's First Factory",
          "colors": "fac"
        },{
          "vis": "pie",
          "dim": "loser_fac2",
          "dim_pretty": "Loser's Second Factory",
          "colors": "fac"
        },{
          "vis": "pie",
          "dim": "loser_fac3",
          "dim_pretty": "Loser's Third Factory",
          "colors": "fac"
        },{
          "vis": "pie",
          "dim": "loser_fac4",
          "dim_pretty": "Loser's Fourth Factory",
          "colors": "fac"
        },{
          "vis": "pie",
          "dim": "loser_fac5",
          "dim_pretty": "Loser's Fifth Factory",
          "colors": "fac"
        }
      ]
    },{
      "type": "med",
      "charts": [
        {
          "vis": "bar",
          "dim": "winner_whr_lead",
          "group": 100,
          "elasticX": true,
          "dim_pretty": "Winner's WHR Advantage",
          "when": "fullwhr",
          "verticalXAxisTicks": true
        },{
          "vis": "bar",
          "dim": "winner_elo_lead",
          "group": 100,
          "elasticX": true,
          "dim_pretty": "Winner's ELO Advantage",
          "verticalXAxisTicks": true
        },{
          "vis": "bar",
          "dim": "winner_whr",
          "group": 100,
          "elasticX": true,
          "dim_pretty": "Winner's WHR Rating",
          "when": "fullwhr",
          "verticalXAxisTicks": true
        },{
          "vis": "bar",
          "dim": "winner_elo",
          "group": 100,
          "elasticX": true,
          "dim_pretty": "Winner's ELO Rating",
          "verticalXAxisTicks": true
        },{
          "vis": "bar",
          "dim": "loser_whr",
          "group": 100,
          "elasticX": true,
          "dim_pretty": "Loser's WHR Rating",
          "when": "fullwhr",
          "verticalXAxisTicks": true
        },{
          "vis": "bar",
          "dim": "loser_elo",
          "group": 100,
          "elasticX": true,
          "dim_pretty": "Loser's ELO Rating",
          "verticalXAxisTicks": true
        }
      ]
    }
  ]
}
